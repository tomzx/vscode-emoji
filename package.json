{
	"name": "emoji",
	"displayName": "Emoji",
	"description": "Emoji support for Windows 7.",
	"version": "0.2.0",
	"publisher": "tomzx",
	"bugs": {
		"url": "https://github.com/tomzx/vscode-emoji/issues"
	},
	"homepage": "https://github.com/tomzx/vscode-emoji/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/tomzx/vscode-emoji.git"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"emoji",
		"emoticons",
		"smiles",
		"unicode",
		"windows 7"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.6"
	},
	"dependencies": {
		"emoji-datasource-emojione": "^4.0.4",
		"emoji-regex": "^6.5.1"
	}
}
